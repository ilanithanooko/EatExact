{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["./server.js"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "development"
      },
      "command": "npm run dev"
    }
  }
  